{
  "name": "repro",
  "version": "0.1.0",
  "scripts": {
    "start": "node tools/run.js",
    "start:watch": "NODE_ENV=development yarn start",
    "start:dev": "NODE_ENV=development PORT=8080 node tools/run.js",
    "build": "NODE_ENV=production node tools/build.js --release",
    "test": "yarn run test:lint && yarn test:unit",
    "test:lint": "eslint src test tools postcss.config.js webpack.config.js",
    "test:lint:quiet": "echo 'Errors only' && eslint src test tools postcss.config.js webpack.config.js --quiet",
    "test:unit": "jest src",
    "clean": "rm -rf node_modules yarn.lock"
  },
